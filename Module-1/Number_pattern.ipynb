    "def number_pattern(n):\n",
    "    \"\"\"Print number pattern\"\"\"\n",
    "    for line in range(0,n):\n",
    "        for num in range(1,line+1):\n",
    "            print(num,end=' ')\n",
    "        print('\\n')    "
