   "source": [
    "def perfect_squares(n):\n",
    "    sqr_list=[]\n",
    "    for i in range(1,n):\n",
    "        sqr=math.pow(i,2)\n",
    "        if int(sqr)/i==i:\n",
    "            sqr_list.append(int(sqr))\n",
    "        else:\n",
    "            continue\n",
    "    return sqr_list      "
   ]
